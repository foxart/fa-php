<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01//EN" "http://www.w3.org/TR/html4/strict.dtd">
<html>
	<head>
		<title>fa-js</title>
		<meta http-equiv="Content-Type" content="text/html; charset=utf-8">
		<meta http-equiv="pragma" content="no-cache">
		<meta name="description" content="">
		<meta name="keywords" content="">
		<meta name="language" content="ru">
		{css}
		{js}
	</head>
	<body class="ff-monospace">
		<div id="container" class="d-t bg-cl-blue-5">
			<div id="container-content" class="d-t">
				<div id="container-header" class="d-tr bg-cl-red-5">
					<div id="container-header-content" class="d-tc">
						<div class="d-t col-12">
							<div class="d-tr">
								<div class="d-tc va-m">
									<div id="menu-header">
										{menu-header}
									</div>
								</div>
							</div>
						</div>
					</div>
				</div>
				<div id="container-body" class="d-tr h-12 bg-cl-white">
					<div id="container-body-content" class="d-tc h-12 va-t">
						{header}
						{breadcrumbs}
						<div class="content">
							{content}
						</div>
						<div class="content">
							<div id="css" class="fl-l col-6">
								{assets-css}
							</div>
							<div id="js" class="fl-r col-6">
								{assets-js}
							</div>
						</div>
					</div>
				</div>
				<div id="container-footer" class="d-tr">
					<div id="container-footer-content" class="d-tc bg-cl-red-5">
						<div class="d-t col-12">
							<div class="d-tr">
								<div class="d-tc va-m">
									<div class="fl-l">
										<span class="fw-b">&copy; foxart</span>
									</div>
									<div id="path" class="fl-r">
